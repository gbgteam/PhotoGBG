@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_LayoutPage1.cshtml";
}

<script src="@Url.Content("~/Script/Tags/TagsModule.js")" type="text/javascript"> </script>

<script type="text/javascript">
    $(document).ready(function () {
        TagsModule.getInstance().init(@(Html.Raw(Json.Encode(Model))),'body',true);
    });
</script>

<div id="body">
    <h2>Index</h2>
    <div class="tabbable tabs-left">
        <ul class="nav nav-tabs" data-bind="foreach: Tags">
            <li><a data-bind="text: Name"></a></li>
        </ul>
    </div>
    <div class="clear">

    </div>
</div>
